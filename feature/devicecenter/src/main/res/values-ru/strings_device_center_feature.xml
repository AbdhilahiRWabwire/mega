    <string name="device_center_list_view_item_status_backup_stopped">Резервное копирование остановлено</string>
    <!-- The name of the Device or Folder Status which signifies that it has exceeded the maximum allowed Transfer quota -->
    <string name="device_center_list_view_item_status_out_of_quota">Превышена квота</string>
    <!-- The name of the Device or Folder Status which signifies that an error has occurred -->
    <string name="device_center_list_view_item_status_error">Ошибка</string>
    <!-- The name of the Device or Folder Status which signifies that it is blocked due to a certain issue -->
    <string name="device_center_list_view_item_status_blocked">Заблокировано</string>
    <!-- The title of the Device Center feature -->
    <string name="device_center_top_app_bar_title">Центр устройств</string>
    <!-- The name of one of the Device Center Bottom Sheet items which takes the User to the Camera uploads feature when clicked -->
    <string name="device_center_bottom_sheet_item_camera_uploads">Загрузки из камеры</string>
    <!-- The enabled Camera uploads status that is shown alongside the "Camera uploads" Device Center Bottom Sheet item -->
    <string name="device_center_bottom_sheet_item_camera_uploads_enabled_status">Включено</string>
    <!-- The disabled Camera uploads status that is shown alongside the "Camera uploads" Device Center Bottom Sheet item -->
    <string name="device_center_bottom_sheet_item_camera_uploads_disabled_status">Отключено</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, opens the "Rename device" Dialog to let the User rename the selected Device -->
    <string name="device_center_bottom_sheet_item_rename">Переименовать</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, takes the User to the Node Information screen to view more information about the Device or Device Folder -->
    <string name="device_center_bottom_sheet_item_info">Информация</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, takes the User to the Backups feature to display the Device or Device Folder -->
    <string name="device_center_bottom_sheet_item_show_in_backups">Показать в «Резервных копиях»</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, takes the User to the Cloud Drive feature to display the Device or Device Folder -->
    <string name="device_center_bottom_sheet_item_show_in_cloud_drive">Показать в Облачном диске</string>
    <!-- The name of the Device or Folder Sub State when its code is NO_SYNC_ERROR -->
    <string name="device_center_list_view_item_sub_state_no_sync_error">No sync error</string>
    <!-- The name of the Device or Folder Sub State when its code is UNSUPPORTED_FILE_SYSTEM -->
    <string name="device_center_list_view_item_sub_state_unsupported_file_system">MEGA can’t sync or backup this folder because the filesystem on your device is not supported.</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_REMOTE_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_remote_type">The folder you chose can’t be synced</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_LOCAL_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_local_type">Файл нельзя синхронизировать отдельно. Нужно настроить синхронизацию из папки.</string>
    <!-- The name of the Device or Folder Sub State when its code is INITIAL_SCAN_FAILED -->
    <string name="device_center_list_view_item_sub_state_initial_scan_failed">Не удалось выполнить первоначальное сканирование. Вам нужно будет повторно включить синхронизацию или резервное копирование из настольного приложения.</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_NODE_NOT_FOUND -->
    <string name="device_center_list_view_item_sub_state_remote_node_not_found">Не удалось найти папку в MEGA, так как она была перемещена или удалена, либо у вас нет доступа.</string>
    <!-- The name of the Device or Folder Sub State when its code is STORAGE_OVERQUOTA -->
    <string name="device_center_list_view_item_sub_state_storage_overquota">Storage quota has reached the limit.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_EXPIRED -->
    <string name="device_center_list_view_item_sub_state_account_expired">Your plan has expired.</string>
    <!-- The name of the Device or Folder Sub State when its code is FOREIGN_TARGET_OVERSTORAGE -->
    <string name="device_center_list_view_item_sub_state_foreign_target_overshare">Folder can’t be synced as user who shared this folder has reached their storage quota.</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_PATH_HAS_CHANGED -->
    <string name="device_center_list_view_item_sub_state_remote_path_has_changed">Не удаётся найти папку MEGA, так как она была перемещена.</string>
    <!-- The name of the Device or Folder Sub State when its code is SHARE_NON_FULL_ACCESS -->
    <string name="device_center_list_view_item_sub_state_share_non_full_access">Папку нельзя синхронизировать, поскольку это общая папка без полного доступа.</string>
    <!-- The name of the Device or Folder Sub State when its code is PUT_NODES_ERROR -->
    <string name="device_center_list_view_item_sub_state_put_nodes_error">Файлы в этой папке невозможно синхронизировать или создать их резервную копию. Вам нужно будет повторно включить синхронизацию или резервное копирование из настольного приложения.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_BELOW_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_below_path">Папку нельзя синхронизировать, так как она уже содержит синхронизируемые папки.</string>
    <!-- The name of the Device or Folder Sub State when its code is VBOXSHAREDFOLDER_UNSUPPORTED -->
    <string name="device_center_list_view_item_sub_state_vboxsharedfolder_unsupported">MEGA can’t sync or backup VirtualBox folders.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_BLOCKED -->
    <string name="device_center_list_view_item_sub_state_account_blocked">Account has been blocked</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_TEMPORARY_ERROR -->
    <string name="device_center_list_view_item_sub_state_unknown_temporary_error">Problem syncing or backing up this folder. Check again later. If issue continues, contact Support.</string>
    <!-- The name of the Device or Folder Sub State when its code is TOO_MANY_ACTION_PACKETS -->
    <string name="device_center_list_view_item_sub_state_too_many_action_packets">Аккаунт перезагружен. Любые пропущенные обновления ваших резервных копий или синхронизаций не были применены.</string>
    <!-- The name of the Device or Folder Sub State when its code is LOGGED_OUT -->
    <string name="device_center_list_view_item_sub_state_logged_out">Синхронизация или резервное копирование остановлены, поскольку вы, похоже, вышли из настольного приложения. Снова войдите в систему в настольном приложении и возобновите синхронизацию или резервное копирование.</string>
    <!-- The name of the Device or Folder Sub State when its code is MISSING_PARENT_NODE -->
    <string name="device_center_list_view_item_sub_state_missing_parent_node">N/A</string>
    <!-- The name of the Device or Folder Sub State when its code is BACKUP_SOURCE_NOT_BELOW_DRIVE -->
    <string name="device_center_list_view_item_sub_state_backup_source_not_below_drive">Не удаётся найти папку на внешнем диске.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_SAME_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_same_path">There’s already a synced folder at the same path</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_MOVE_CLOUD_NODES -->
    <string name="device_center_list_view_item_sub_state_could_not_move_cloud_nodes">Не удалось переименовать.</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_CREATE_IGNORE_FILE -->
    <string name="device_center_list_view_item_sub_state_could_not_create_ignore_file">Couldn’t create a sync. File has been ignored.</string>
    <!-- The name of the Device or Folder Sub State when its code is SYNC_CONFIG_READ_FAILURE -->
    <string name="device_center_list_view_item_sub_state_sync_config_read_failure">Couldn’t read sync configs from drive.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_DRIVE_PATH -->
    <string name="device_center_list_view_item_sub_state_unknown_drive_path">Sync’s drive path is unknown</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_SCAN_INTERVAL -->
    <string name="device_center_list_view_item_sub_state_invalid_scan_interval">Invalid scan interval</string>
    <!-- The name of the Device or Folder Sub State when its code is NOTIFICATION_SYSTEM_UNAVAILABLE -->
    <string name="device_center_list_view_item_sub_state_notification_system_unavailable">Filesystem notification subsystem has encountered an unrecoverable error.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_ADD_WATCH -->
    <string name="device_center_list_view_item_sub_state_unable_to_add_watch">Unable to add a filesystem watch.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_RETRIEVE_ROOT_FSID -->
    <string name="device_center_list_view_item_sub_state_unable_to_retrieve_root_fsid">Unable to retrieve a sync root’s FSID.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_OPEN_DATABASE -->
    <string name="device_center_list_view_item_sub_state_unable_to_open_database">Unable to open state cache database</string>
    <!-- The name of the Device or Folder Sub State when its code is INSUFFICIENT_DISK_SPACE -->
    <string name="device_center_list_view_item_sub_state_insufficient_disk_space">Недостаточно места для скачивания.</string>
    <!-- The name of the Device or Folder Sub State when its code is FAILURE_ACCESSING_PERSISTENT_STORAGE -->
    <string name="device_center_list_view_item_sub_state_failure_accessing_persistent_storage">No access to persistent storage</string>
    <!-- The name for specific Sub States to indicate that an unknown error occurred -->
    <string name="device_center_list_view_item_sub_state_message_unknown_error">Неизвестная ошибка.</string>
    <!-- The name for specific Sub States to indicate that something went wrong -->
    <string name="device_center_list_view_item_sub_state_message_something_went_wrong">Something went wrong.</string>
    <!-- The name for specific Sub States to indicate that the Node is in the Rubbish Bin -->
    <string name="device_center_list_view_item_sub_state_message_node_in_rubbish_bin">Папку нельзя синхронизировать или скопировать, так как папка MEGA находится в Корзине.</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located at this time -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive_now">Не удалось найти папку на вашем устройстве. Повторите попытку позже.</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive">Folder in your device can’t be located</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder due to some changes -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_recent_changes">Problem syncing or backing up this folder because of changes to the MEGA folder. Stop the backup and try setting it up again in the desktop app, or contact Support.</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue">Problem syncing or backing up this folder. Stop the sync or backup and try setting it up again in the desktop app, or contact Support.</string>
    <!-- The name for specific Sub States to indicate that the MEGA Folder cannot be synced or backed up as it is inside another synced MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_being_inside_another_backed_up_folder">Папку нельзя синхронизировать, так как она уже находится внутри синхронизируемой папки.</string>
    <!-- The name of the Device Status informing the User that Camera Uploads is disabled for the Current Device -->
    <string name="device_center_list_view_item_status_camera_uploads_disabled">Загрузки из камеры отключены</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits an empty Device Name -->
    <string name="device_center_rename_device_dialog_error_message_empty_device_name">Введите название устройства</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name that already exists in the current list of Devices -->
    <string name="device_center_rename_device_dialog_error_message_name_already_exists">Устройство с таким названием уже существует. Введите другое название.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name with invalid characters -->
    <string name="device_center_rename_device_dialog_error_message_invalid_characters">Следующие символы не допускаются: %1$s</string>
    <!-- The Snackbar message to inform the User that the Device has been successfully renamed -->
    <string name="device_center_snackbar_message_rename_device_successful">Устройство переименовано</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name whose length is greater than 32 characters -->
    <string name="device_center_rename_device_dialog_error_message_maximum_character_length_exceeded">Максимум 32 символа</string>
