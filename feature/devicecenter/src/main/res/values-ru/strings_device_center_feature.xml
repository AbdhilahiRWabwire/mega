    <!-- The name of the Header in Device Center which signifies the User's Devices that are not currently in use  -->
    <string name="device_center_list_view_item_header_other_devices">Другие устройства</string>
    <!-- The name of the Device or Folder Status when no matching Status is found-->
    <string name="device_center_list_view_item_status_unknown_status">Неизвестный статус</string>
    <!-- The name of the Device if the SDK cannot retrieve the name for that Device -->
    <string name="device_center_list_view_item_title_unknown_device">Неизвестное устройство</string>
    <!-- The name of the Device or Folder Status which signifies that it is Up to Date -->
    <string name="device_center_list_view_item_status_up_to_date">Синхронизировано</string>
