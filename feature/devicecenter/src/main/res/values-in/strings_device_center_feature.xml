    <string name="device_center_list_view_item_status_no_camera_uploads">Tidak ada unggahan kamera</string>
    <!-- The name of the Device or Folder Status which signifies that it is disabled -->
    <string name="device_center_list_view_item_status_disabled">Membatalkan</string>
    <!-- The name of the Device or Folder Status which signifies that it is offline -->
    <string name="device_center_list_view_item_status_offline">Tidak Aktif</string>
