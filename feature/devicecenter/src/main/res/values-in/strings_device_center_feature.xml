    <string name="device_center_list_view_item_status_initialising">Memulai&#8230;</string>
    <!-- The name of the Device or Folder Status which signifies that it is currently scanning -->
    <string name="device_center_list_view_item_status_scanning">Memindai&#8230;</string>
    <!-- The name of the Device or Folder Status which signifies that it is currently updating its backup contents -->
    <string name="device_center_list_view_item_status_updating">Memperbarui&#8230;</string>
