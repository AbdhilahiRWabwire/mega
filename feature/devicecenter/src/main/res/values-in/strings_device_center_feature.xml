    <string name="device_center_list_view_item_status_paused">Berhenti sementara</string>
    <!-- The name of the Device or Folder Status which signifies that the backup procedure has been stopped -->
    <string name="device_center_list_view_item_status_backup_stopped">Pencadangan dihentikan</string>
    <!-- The name of the Device or Folder Status which signifies that it has exceeded the maximum allowed Transfer quota -->
    <string name="device_center_list_view_item_status_out_of_quota">Habis kuota</string>
    <!-- The name of the Device or Folder Status which signifies that an error has occurred -->
    <string name="device_center_list_view_item_status_error">Kesalahan</string>
    <!-- The name of the Device or Folder Status which signifies that it is blocked due to a certain issue -->
    <string name="device_center_list_view_item_status_blocked">Terblokir</string>
    <!-- The title of the Device Center feature -->
    <string name="device_center_top_app_bar_title">Pusat perangkat</string>
    <!-- The name of one of the Device Center Bottom Sheet items which takes the User to the Camera uploads feature when clicked -->
    <string name="device_center_bottom_sheet_item_camera_uploads">Unggahan camera</string>
    <!-- The enabled Camera uploads status that is shown alongside the "Camera uploads" Device Center Bottom Sheet item -->
    <string name="device_center_bottom_sheet_item_camera_uploads_enabled_status">Diaktifkan</string>
    <!-- The disabled Camera uploads status that is shown alongside the "Camera uploads" Device Center Bottom Sheet item -->
    <string name="device_center_bottom_sheet_item_camera_uploads_disabled_status">Membatalkan</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, opens the "Rename device" Dialog to let the User rename the selected Device -->
    <string name="device_center_bottom_sheet_item_rename">Ubah nama</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, takes the User to the Node Information screen to view more information about the Device or Device Folder -->
    <string name="device_center_bottom_sheet_item_info">Info</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, takes the User to the Backups feature to display the Device or Device Folder -->
    <string name="device_center_bottom_sheet_item_show_in_backups">Tampilkan di Cadangan</string>
    <!-- The name of one of the Device Center Bottom Sheet items that when clicked, takes the User to the Cloud Drive feature to display the Device or Device Folder -->
    <string name="device_center_bottom_sheet_item_show_in_cloud_drive">Tunjukkan di Cloud Drive</string>
    <!-- The name of the Device or Folder Sub State when its code is NO_SYNC_ERROR -->
    <string name="device_center_list_view_item_sub_state_no_sync_error">No sync error</string>
    <!-- The name of the Device or Folder Sub State when its code is UNSUPPORTED_FILE_SYSTEM -->
    <string name="device_center_list_view_item_sub_state_unsupported_file_system">MEGA can’t sync or backup this folder because the file system on your device is not supported</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_REMOTE_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_remote_type">The folder you chose can’t be synced</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_LOCAL_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_local_type">File tidak dapat disinkronkan satu per satu. Anda perlu mengatur sinkronisasi dari folder.</string>
    <!-- The name of the Device or Folder Sub State when its code is INITIAL_SCAN_FAILED -->
    <string name="device_center_list_view_item_sub_state_initial_scan_failed">Pemindaian awal gagal. Anda harus mengaktifkan kembali sinkronisasi atau cadangan dari aplikasi desktop.</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_NODE_NOT_FOUND -->
    <string name="device_center_list_view_item_sub_state_remote_node_not_found">Folder di MEGA tidak dapat ditemukan karena telah dipindahkan atau dihapus, atau anda mungkin tidak memiliki akses.</string>
    <!-- The name of the Device or Folder Sub State when its code is STORAGE_OVERQUOTA -->
    <string name="device_center_list_view_item_sub_state_storage_overquota">Batas kuota penyimpanan tercapai.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_EXPIRED -->
    <string name="device_center_list_view_item_sub_state_account_expired">Unable to sync or back up this folder as your plan has expired</string>
    <!-- The name of the Device or Folder Sub State when its code is FOREIGN_TARGET_OVERSTORAGE -->
    <string name="device_center_list_view_item_sub_state_foreign_target_overshare">Folder can’t be synced as the user who shared this folder has reached their storage quota</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_PATH_HAS_CHANGED -->
    <string name="device_center_list_view_item_sub_state_remote_path_has_changed">Folder di MEGA tidak dapat ditemukan karena telah dipindahkan atau dihapus, atau anda mungkin tidak memiliki akses.</string>
    <!-- The name of the Device or Folder Sub State when its code is SHARE_NON_FULL_ACCESS -->
    <string name="device_center_list_view_item_sub_state_share_non_full_access">Folder can’t be synced as it’s a shared folder without full access</string>
    <!-- The name of the Device or Folder Sub State when its code is PUT_NODES_ERROR -->
    <string name="device_center_list_view_item_sub_state_put_nodes_error">Files in this folder can’t be synced or backed up. You will need to re-enable the sync or backup from the desktop app.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_BELOW_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_below_path">Folder can’t be synced as it already contains synced folders</string>
    <!-- The name of the Device or Folder Sub State when its code is VBOXSHAREDFOLDER_UNSUPPORTED -->
    <string name="device_center_list_view_item_sub_state_vboxsharedfolder_unsupported">MEGA can’t sync or back up VirtualBox folders</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_BLOCKED -->
    <string name="device_center_list_view_item_sub_state_account_blocked">Unable to sync or back up this folder as the account has been blocked</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_TEMPORARY_ERROR -->
    <string name="device_center_list_view_item_sub_state_unknown_temporary_error">Problem syncing or backing up this folder. Try again later. If the issue continues, contact support.</string>
    <!-- The name of the Device or Folder Sub State when its code is TOO_MANY_ACTION_PACKETS -->
    <string name="device_center_list_view_item_sub_state_too_many_action_packets">Akun dimuat ulang. Pembaruan apa pun yang terlewat untuk cadangan atau sinkronisasi anda belum diterapkan.</string>
    <!-- The name of the Device or Folder Sub State when its code is LOGGED_OUT -->
    <string name="device_center_list_view_item_sub_state_logged_out">Sync or backup has been stopped as you appear to have logged out of the desktop app. Log back in via the desktop app, and resume the sync or backup.</string>
    <!-- The name of the Device or Folder Sub State when its code is MISSING_PARENT_NODE -->
    <string name="device_center_list_view_item_sub_state_missing_parent_node">N/A</string>
    <!-- The name of the Device or Folder Sub State when its code is BACKUP_SOURCE_NOT_BELOW_DRIVE -->
    <string name="device_center_list_view_item_sub_state_backup_source_not_below_drive">Folder di drive eksternal tidak dapat ditemukan.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_SAME_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_same_path">There’s already a synced folder at the same path</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_MOVE_CLOUD_NODES -->
    <string name="device_center_list_view_item_sub_state_could_not_move_cloud_nodes">Mengganti nama gagal.</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_CREATE_IGNORE_FILE -->
    <string name="device_center_list_view_item_sub_state_could_not_create_ignore_file">Couldn’t create a .megaignore file for this sync</string>
    <!-- The name of the Device or Folder Sub State when its code is SYNC_CONFIG_READ_FAILURE -->
    <string name="device_center_list_view_item_sub_state_sync_config_read_failure">Couldn’t read sync configuration. Try again later or check folder permissions.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_DRIVE_PATH -->
    <string name="device_center_list_view_item_sub_state_unknown_drive_path">Sync folder location is unknown</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_SCAN_INTERVAL -->
    <string name="device_center_list_view_item_sub_state_invalid_scan_interval">Interval pemindaian tidak valid. Periksa pengaturan interval pemindaian dan coba lagi.</string>
    <!-- The name of the Device or Folder Sub State when its code is NOTIFICATION_SYSTEM_UNAVAILABLE -->
    <string name="device_center_list_view_item_sub_state_notification_system_unavailable">Tidak dapat berkomunikasi dengan lokasi folder. Periksa lokasi dapat diakses dan izin untuk lokasi folder diberikan.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_ADD_WATCH -->
    <string name="device_center_list_view_item_sub_state_unable_to_add_watch">Tidak dapat menambahkan pengawasan sistem file. Pastikan ada ruang kosong dan memori yang cukup, dan bahwa anda telah memberikan izin untuk lokasi folder.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_RETRIEVE_ROOT_FSID -->
    <string name="device_center_list_view_item_sub_state_unable_to_retrieve_root_fsid">Tidak dapat membaca lokasi sinkronisasi. Periksa lokasi dapat diakses dan izin untuk lokasi folder diberikan.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_OPEN_DATABASE -->
    <string name="device_center_list_view_item_sub_state_unable_to_open_database">Unable to open state cache database</string>
    <!-- The name of the Device or Folder Sub State when its code is INSUFFICIENT_DISK_SPACE -->
    <string name="device_center_list_view_item_sub_state_insufficient_disk_space">Ruang tidak mencukupi untuk mengunduh.</string>
    <!-- The name of the Device or Folder Sub State when its code is FAILURE_ACCESSING_PERSISTENT_STORAGE -->
    <string name="device_center_list_view_item_sub_state_failure_accessing_persistent_storage">Tidak dapat membaca lokasi sinkronisasi. Periksa lokasi dapat diakses dan izin untuk lokasi folder diberikan.</string>
    <!-- The name for specific Sub States to indicate that an unknown error occurred -->
    <string name="device_center_list_view_item_sub_state_message_unknown_error">Kesalahan tidak diketahui.</string>
    <!-- The name for specific Sub States to indicate that something went wrong -->
    <string name="device_center_list_view_item_sub_state_message_something_went_wrong">Something went wrong.</string>
    <!-- The name for specific Sub States to indicate that the Node is in the Rubbish Bin -->
    <string name="device_center_list_view_item_sub_state_message_node_in_rubbish_bin">Folder can’t be synced or backed up as the MEGA folder is in the Rubbish bin</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located at this time -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive_now">Folder di perangkat anda tidak dapat ditemukan sekarang, coba lagi nanti.</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive">Folder in your device can’t be located</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder due to some changes -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_recent_changes">Problem syncing or backing up this folder because of changes to the MEGA folder. Stop the sync or backup and try setting it up again in the desktop app, or contact support.</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue">Masalah saat menyinkronkan atau membuat cadangan folder ini. Hentikan sinkronisasi atau pencadangan dan coba atur lagi di aplikasi Desktop, atau hubungi Bantuan.</string>
    <!-- The name for specific Sub States to indicate that the MEGA Folder cannot be synced or backed up as it is inside another synced MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_being_inside_another_backed_up_folder">Folder can’t be synced as it’s already inside a synced folder</string>
    <!-- The name of the Device Status informing the User that Camera Uploads is disabled for the Current Device -->
    <string name="device_center_list_view_item_status_camera_uploads_disabled">Camera uploads disabled</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits an empty Device Name -->
    <string name="device_center_rename_device_dialog_error_message_empty_device_name">Enter a device name.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name that already exists in the current list of Devices -->
    <string name="device_center_rename_device_dialog_error_message_name_already_exists">Perangkat dengan nama ini sudah ada. Masukkan nama yang berbeda.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name with invalid characters -->
    <string name="device_center_rename_device_dialog_error_message_invalid_characters">Karakter berikut tidak diperbolehkan: %1$s</string>
    <!-- The Snackbar message to inform the User that the Device has been successfully renamed -->
    <string name="device_center_snackbar_message_rename_device_successful">Device renamed</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name whose length is greater than 32 characters -->
    <string name="device_center_rename_device_dialog_error_message_maximum_character_length_exceeded">Maximum 32 characters</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the Camera uploads feature settings -->
    <string name="device_center_device_top_menu_option_camera_uploads">Unggahan camera</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which opens the dialog to rename the selected Device -->
    <string name="device_center_device_top_menu_option_rename">Ubah nama</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the device info screen -->
    <string name="device_center_device_top_menu_option_info">Info</string>
    <!-- Text to show in the Device Center for a No Network State when there is no network connectivity -->
    <string name="device_center_no_network_state">Tidak ada koneksi internet</string>
    <!-- The name of the Device or Folder Sub State when its code is LOCAL_FILESYSTEM_MISMATCH -->
    <string name="device_center_list_view_item_sub_state_local_filesystem_mismatch">Files in this folder can’t be synced or backed up. You will need to re-enable the sync or backup from the desktop app.</string>
    <!-- Text to display in Device Center for an empty state when inspecting a device where no sync or backup has been configured -->
    <string name="device_center_nothing_setup_state">Belum ada yang disiapkan</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Devices -->
    <string name="device_center_top_app_bar_search_devices_hint">Search devices</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Syncs -->
    <string name="device_center_top_app_bar_search_syncs_hint">Search syncs</string>
    <!-- Text to show in the Device Center when no search results are found -->
    <string name="device_center_empty_screen_no_results">Tidak Ada Hasil</string>
    <!-- Header of the Info view field for what is the content of a device/folder -->
    <string name="device_center_info_content">Memiliki</string>
    <!-- Header of the Info view field for what is the total size of the content of a device/folder -->
    <string name="device_center_info_total_size">Ukuran total</string>
    <!-- String which indicates the number of files contained into a device/folder -->
    <plurals name="device_center_info_num_files">
        <item quantity="other">%1$d berkas</item>
    </plurals>
    <!-- String which indicates the number of folders contained into a device/folder -->
    <plurals name="device_center_info_num_folders">
        <item quantity="one">%1$d folder</item>
        <item quantity="other">%1$d folders</item>
    </plurals>
    <!-- First part of string, which indicates the content of a device/folder is some folders and some files. The full string is '%1$d folders · %2$d files'. The string was split in 2 for pluralization. Middle height point is to separate two fragments of text and it was not to be considered a punctuation mark. Please keep the spaces around middle height point. -->
    <plurals name="device_center_info_num_folders_and_files">
        <item quantity="other">%1$d folder&#160;·&#160;</item>
    </plurals>
    <!-- Header of the Info view field for the date and time a folder was added -->
    <string name="device_center_info_added">Ditambahkan</string>
</resources>