    <string name="device_center_rename_device_dialog_error_message_name_already_exists">Ein Gerät mit diesem Namen existiert bereits. Bitte geben Sie einen anderen Namen ein.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name with invalid characters -->
    <string name="device_center_rename_device_dialog_error_message_invalid_characters">Die folgenden Zeichen sind nicht zulässig: %1$s</string>
    <!-- The Snackbar message to inform the User that the Device has been successfully renamed -->
    <string name="device_center_snackbar_message_rename_device_successful">Gerät umbenannt</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name whose length is greater than 32 characters -->
    <string name="device_center_rename_device_dialog_error_message_maximum_character_length_exceeded">Max. 32 Zeichen</string>
