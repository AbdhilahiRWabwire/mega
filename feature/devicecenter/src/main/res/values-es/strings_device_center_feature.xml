    <!-- The name of the Device or Folder Sub State when its code is NO_SYNC_ERROR -->
    <string name="device_center_list_view_item_sub_state_no_sync_error">No sync error</string>
    <!-- The name of the Device or Folder Sub State when its code is UNSUPPORTED_FILE_SYSTEM -->
    <string name="device_center_list_view_item_sub_state_unsupported_file_system">No se puede sincronizar ni hacer un backup de esta carpeta ya que el sistema de archivos de tu dispositivo no es compatible</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_REMOTE_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_remote_type">La carpeta que has elegido no se puede sincronizar</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_LOCAL_TYPE -->
    <string name="device_center_list_view_item_sub_state_invalid_local_type">No se puede sincronizar un archivo de forma individual. Debes configurar la sincronización desde una carpeta.</string>
    <!-- The name of the Device or Folder Sub State when its code is INITIAL_SCAN_FAILED -->
    <string name="device_center_list_view_item_sub_state_initial_scan_failed">El escaneo ha fallado. Vuelve a activar la sincronización o el backup desde la aplicación de escritorio.</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_NODE_NOT_FOUND -->
    <string name="device_center_list_view_item_sub_state_remote_node_not_found">No se ha podido localizar la carpeta en MEGA porque se ha movido o eliminado, o puede que no tengas acceso a ella.</string>
    <!-- The name of the Device or Folder Sub State when its code is STORAGE_OVERQUOTA -->
    <string name="device_center_list_view_item_sub_state_storage_overquota">Se ha alcanzado el límite de cuota de almacenamiento.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_EXPIRED -->
    <string name="device_center_list_view_item_sub_state_account_expired">No se ha podido sincronizar ni hacer un backup de esta carpeta porque tu plan ha caducado</string>
    <!-- The name of the Device or Folder Sub State when its code is FOREIGN_TARGET_OVERSTORAGE -->
    <string name="device_center_list_view_item_sub_state_foreign_target_overshare">La carpeta no se puede sincronizar porque el usuario que la ha compartido ha alcanzado el límite de su cuota de almacenamiento</string>
    <!-- The name of the Device or Folder Sub State when its code is REMOTE_PATH_HAS_CHANGED -->
    <string name="device_center_list_view_item_sub_state_remote_path_has_changed">No se ha podido localizar la carpeta en MEGA porque se ha movido o eliminado, o puede que no tengas acceso a ella.</string>
    <!-- The name of the Device or Folder Sub State when its code is SHARE_NON_FULL_ACCESS -->
    <string name="device_center_list_view_item_sub_state_share_non_full_access">La carpeta no se puede sincronizar porque es una carpeta compartida sin acceso completo</string>
    <!-- The name of the Device or Folder Sub State when its code is PUT_NODES_ERROR -->
    <string name="device_center_list_view_item_sub_state_put_nodes_error">Los archivos de esta carpeta no se pueden sincronizar ni se puede hacer un backup. Reactiva la sincronización o el backup desde la aplicación de escritorio.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_BELOW_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_below_path">La carpeta no se puede sincronizar porque ya contiene carpetas sincronizadas</string>
    <!-- The name of the Device or Folder Sub State when its code is VBOXSHAREDFOLDER_UNSUPPORTED -->
    <string name="device_center_list_view_item_sub_state_vboxsharedfolder_unsupported">MEGA no puede sincronizar ni hacer backups de las carpetas de VirtualBox</string>
    <!-- The name of the Device or Folder Sub State when its code is ACCOUNT_BLOCKED -->
    <string name="device_center_list_view_item_sub_state_account_blocked">No se puede sincronizar ni hacer backups de esta carpeta porque la cuenta está bloqueada</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_TEMPORARY_ERROR -->
    <string name="device_center_list_view_item_sub_state_unknown_temporary_error">Se ha producido un problema al sincronizar o hacer un backup de esta carpeta. Vuelve a intentarlo más tarde. Si el problema persiste, ponte en contacto con el servicio de soporte.</string>
    <!-- The name of the Device or Folder Sub State when its code is TOO_MANY_ACTION_PACKETS -->
    <string name="device_center_list_view_item_sub_state_too_many_action_packets">Cuenta recargada. Las actualizaciones pendientes de tus backups o sincronizaciones no se han aplicado.</string>
    <!-- The name of the Device or Folder Sub State when its code is LOGGED_OUT -->
    <string name="device_center_list_view_item_sub_state_logged_out">La sincronización o el backup se han detenido porque se ha cerrado la sesión en la aplicación de escritorio. Vuelve a iniciar sesión a través de la aplicación de escritorio y reanuda la operación.</string>
    <!-- The name of the Device or Folder Sub State when its code is MISSING_PARENT_NODE -->
    <string name="device_center_list_view_item_sub_state_missing_parent_node">N/A</string>
    <!-- The name of the Device or Folder Sub State when its code is BACKUP_SOURCE_NOT_BELOW_DRIVE -->
    <string name="device_center_list_view_item_sub_state_backup_source_not_below_drive">No es posible encontrar la carpeta en la unidad externa.</string>
    <!-- The name of the Device or Folder Sub State when its code is ACTIVE_SYNC_SAME_PATH -->
    <string name="device_center_list_view_item_sub_state_active_sync_same_path">Ya existe una carpeta sincronizada en la misma ruta</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_MOVE_CLOUD_NODES -->
    <string name="device_center_list_view_item_sub_state_could_not_move_cloud_nodes">Error al cambiar el nombre.</string>
    <!-- The name of the Device or Folder Sub State when its code is COULD_NOT_CREATE_IGNORE_FILE -->
    <string name="device_center_list_view_item_sub_state_could_not_create_ignore_file">No se ha podido crear un archivo .megaignore para esta sincronización</string>
    <!-- The name of the Device or Folder Sub State when its code is SYNC_CONFIG_READ_FAILURE -->
    <string name="device_center_list_view_item_sub_state_sync_config_read_failure">No se ha podido leer la configuración de sincronización. Inténtalo más tarde o comprueba los permisos de la carpeta.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNKNOWN_DRIVE_PATH -->
    <string name="device_center_list_view_item_sub_state_unknown_drive_path">Se desconoce la ubicación de la carpeta de sincronización</string>
    <!-- The name of the Device or Folder Sub State when its code is INVALID_SCAN_INTERVAL -->
    <string name="device_center_list_view_item_sub_state_invalid_scan_interval">Intervalo de escaneo no válido. Comprueba la configuración del intervalo de escaneo e inténtalo de nuevo.</string>
    <!-- The name of the Device or Folder Sub State when its code is NOTIFICATION_SYSTEM_UNAVAILABLE -->
    <string name="device_center_list_view_item_sub_state_notification_system_unavailable">No se ha podido comunicar con la ubicación de la carpeta. Comprueba que se pueda acceder a la ubicación y que se hayan concedido los permisos para la ubicación de la carpeta.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_ADD_WATCH -->
    <string name="device_center_list_view_item_sub_state_unable_to_add_watch">No se puede añadir una vigilancia del sistema de archivos. Asegúrate de que haya suficiente espacio libre y suficiente memoria y de que los permisos para la ubicación de la carpeta estén concedidos.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_RETRIEVE_ROOT_FSID -->
    <string name="device_center_list_view_item_sub_state_unable_to_retrieve_root_fsid">No se ha podido leer la ubicación de la sincronización. Comprueba que se pueda acceder a la ubicación y que se hayan concedido los permisos para la ubicación de la carpeta.</string>
    <!-- The name of the Device or Folder Sub State when its code is UNABLE_TO_OPEN_DATABASE -->
    <string name="device_center_list_view_item_sub_state_unable_to_open_database">Unable to open state cache database</string>
    <!-- The name of the Device or Folder Sub State when its code is INSUFFICIENT_DISK_SPACE -->
    <string name="device_center_list_view_item_sub_state_insufficient_disk_space">Espacio insuficiente para la descarga.</string>
    <!-- The name of the Device or Folder Sub State when its code is FAILURE_ACCESSING_PERSISTENT_STORAGE -->
    <string name="device_center_list_view_item_sub_state_failure_accessing_persistent_storage">No se ha podido leer la ubicación de la sincronización. Comprueba que se pueda acceder a la ubicación y que se hayan concedido los permisos para la ubicación de la carpeta.</string>
    <!-- The name for specific Sub States to indicate that an unknown error occurred -->
    <string name="device_center_list_view_item_sub_state_message_unknown_error">Error desconocido.</string>
    <!-- The name for specific Sub States to indicate that something went wrong -->
    <string name="device_center_list_view_item_sub_state_message_something_went_wrong">Algo ha salido mal</string>
    <!-- The name for specific Sub States to indicate that the Node is in the Rubbish Bin -->
    <string name="device_center_list_view_item_sub_state_message_node_in_rubbish_bin">Los archivos de esta carpeta no se pueden sincronizar ni se puede hacer un backup porque la carpeta de MEGA está en la Papelera</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located at this time -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive_now">No se ha podido localizar la carpeta de tu dispositivo, inténtalo de nuevo más tarde.</string>
    <!-- The name for specific Sub States to indicate that the local drive cannot be located -->
    <string name="device_center_list_view_item_sub_state_message_cannot_locate_local_drive">No se ha podido localizar la carpeta de tu dispositivo</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder due to some changes -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_recent_changes">Ha habido un problema al sincronizar o hacer un backup de esta carpeta debido a unos cambios en la carpeta de MEGA. Detén la sincronización o el backup y actívalos de nuevo desde la aplicación de escritorio o contacta con el servicio de soporte.</string>
    <!-- The name for specific Sub States to indicate that a problem occurred when syncing or backing up the MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue">Se ha producido un problema al sincronizar o hacer un backup de esta carpeta. Detén la sincronización o el backup e intenta volver a configurarlos en la aplicación de escritorio, o ponte en contacto con el servicio de soporte.</string>
    <!-- The name for specific Sub States to indicate that the MEGA Folder cannot be synced or backed up as it is inside another synced MEGA Folder -->
    <string name="device_center_list_view_item_sub_state_message_folder_backup_issue_due_to_being_inside_another_backed_up_folder">La carpeta no se puede sincronizar porque ya está dentro de una carpeta sincronizada</string>
    <!-- The name of the Device Status informing the User that Camera Uploads is disabled for the Current Device -->
    <string name="device_center_list_view_item_status_camera_uploads_disabled">Subidas de la cámara desactivadas</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits an empty Device Name -->
    <string name="device_center_rename_device_dialog_error_message_empty_device_name">Introducir el nombre del dispositivo.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name that already exists in the current list of Devices -->
    <string name="device_center_rename_device_dialog_error_message_name_already_exists">Ya existe un dispositivo con este nombre. Introduce un nombre diferente.</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name with invalid characters -->
    <string name="device_center_rename_device_dialog_error_message_invalid_characters">Los siguientes caracteres no están permitidos: %1$s</string>
    <!-- The Snackbar message to inform the User that the Device has been successfully renamed -->
    <string name="device_center_snackbar_message_rename_device_successful">Dispositivo renombrado</string>
    <!-- The Error Message shown in the Rename Device Dialog when the User submits a new Device Name whose length is greater than 32 characters -->
    <string name="device_center_rename_device_dialog_error_message_maximum_character_length_exceeded">Máximo 32 caracteres</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the Camera uploads feature settings -->
    <string name="device_center_device_top_menu_option_camera_uploads">Subidas de la cámara</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which opens the dialog to rename the selected Device -->
    <string name="device_center_device_top_menu_option_rename">Renombrar</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the device info screen -->
    <string name="device_center_device_top_menu_option_info">Información</string>
    <!-- Text to show in the Device Center for a No Network State when there is no network connectivity -->
    <string name="device_center_no_network_state">No hay conexión a internet</string>
    <!-- The name of the Device or Folder Sub State when its code is LOCAL_FILESYSTEM_MISMATCH -->
    <string name="device_center_list_view_item_sub_state_local_filesystem_mismatch">Los archivos de esta carpeta no se pueden sincronizar ni se puede hacer un backup. Reactiva la sincronización o el backup desde la aplicación de escritorio.</string>
    <!-- Text to display in Device Center for an empty state when inspecting a device where no sync or backup has been configured -->
    <string name="device_center_nothing_setup_state">Aún no hay nada configurado</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Devices -->
    <string name="device_center_top_app_bar_search_devices_hint">Buscar dispositivos</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Syncs -->
    <string name="device_center_top_app_bar_search_syncs_hint">Buscar sincronizaciones</string>
    <!-- Text to show in the Device Center when no search results are found -->
    <string name="device_center_empty_screen_no_results">No hay resultados</string>
    <!-- Header of the Info view field for what is the content of a device/folder -->
    <string name="device_center_info_content">Contiene</string>
    <!-- Header of the Info view field for what is the total size of the content of a device/folder -->
    <string name="device_center_info_total_size">Tamaño total</string>
    <!-- String which indicates the number of files contained into a device/folder -->
    <plurals name="device_center_info_num_files">
        <item quantity="one">%1$d archivo</item>
        <item quantity="many">%1$dde archivos</item>
        <item quantity="other">%1$d archivos</item>
    </plurals>
    <!-- String which indicates the number of folders contained into a device/folder -->
    <plurals name="device_center_info_num_folders">
        <item quantity="one">%1$d carpeta</item>
        <item quantity="many">%1$d de carpetas</item>
        <item quantity="other">%1$d carpetas</item>
    </plurals>
    <!-- First part of string, which indicates the content of a device/folder is some folders and some files. The full string is '%1$d folders · %2$d files'. The string was split in 2 for pluralization. Middle height point is to separate two fragments of text and it was not to be considered a punctuation mark. Please keep the spaces around middle height point. -->
    <plurals name="device_center_info_num_folders_and_files">
        <item quantity="one">%1$d carpeta&#160;·&#160;</item>
        <item quantity="many">%1$d de carpetas&#160;·&#160;</item>
        <item quantity="other">%1$d carpetas&#160;·&#160;</item>
    </plurals>
    <!-- Header of the Info view field for the date and time a folder was added -->
    <string name="device_center_info_added">Añadidas</string>
</resources>